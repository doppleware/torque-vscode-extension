{
  "name": "torque",
  "displayName": "Torque AI",
  "description": "Autonomous identification, root cause analysis and remediation of code and infrastructure issues",
  "version": "0.6.0",
  "publisher": "Quali",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.94.0"
  },
  "keywords": [
    "OpenTelemetry",
    "Observability",
    "IntelliSense",
    "IntelliCode",
    "Flask",
    "FastAPI",
    "ASP.NET",
    "C#",
    "eBPF",
    "Jaeger",
    "Python",
    "dotnet",
    "Continuous Delivery",
    "DevOps"
  ],
  "badges": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doppleware/torque-vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/doppleware/torque-vscode-extension/issues"
  },
  "homepage": "https://github.com/doppleware/torque-vscode-extension",
  "categories": [
    "Programming Languages",
    "Visualization",
    "Linters",
    "Debuggers"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "torque.url": {
          "type": "string",
          "title": "URL",
          "default": "https://localhost:5051",
          "description": "API URL",
          "order": 0
        },
        "torque.token": {
          "type": "string",
          "default": "",
          "description": "API token",
          "order": 1
        },
        "torque.login": {
          "type": "string",
          "description": "User login",
          "order": 2
        },
        "torque.password": {
          "type": "string",
          "description": "User password",
          "order": 3
        },
        "torque.copySettingsToMcp": {
          "type": "boolean",
          "title": "Copy settings to MCP",
          "default": false,
          "description": "Copy settings to MCP server",
          "order": 4
        }
      }
    }
  },
  "scripts": {
    "clean": "rimraf ./out",
    "build:dev": "npm run clean && node esbuild.ts",
    "build:prod": "npm run clean && node esbuild.ts --production",
    "check-types": "tsc --noEmit",
    "compile:dev": "npm run check-types && npm run build:dev",
    "compile:prod": "npm run check-types && npm run build:prod",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.ts --watch",
    "watch:tsc": "tsc --noEmit -w",
    "lint": "eslint --cache .",
    "fix:prettier": "prettier --write './**/*.{json,md,ts}'",
    "precommit": "lint-staged",
    "prepare": "husky",
    "vscode:prepublish": "npm run compile:prod",
    "package": "vsce package",
    "test": "vscode-test",
    "pretest": "npm run compile:dev && npm run lint"
  },
  "lint-staged": {
    "*.{json,ts}": [
      "eslint --cache"
    ],
    "*.{json,md,ts}": [
      "prettier --write"
    ]
  },
  "__metadata": {
    "id": "2e214876-5bea-4ee6-8026-222de5313d92",
    "publisherDisplayName": "Quali",
    "publisherId": "caa21fe0-eb8b-43fd-9197-ae8fb8b1b5df",
    "isPreReleaseVersion": false
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.94.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "audit-ci": "^7.1.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.4",
    "mocha": "^11.7.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "path-to-regexp": "^8.2.0"
  }
}
